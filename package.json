{
    "name": "sunshine",
    "version": "0.0.1",
    "scripts": {
        "build:prod": "ng build --configuration production --base-href https://eyzim.github.io/sunshine/",
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "pkg": "pkg server.js -t win-x64 --config package.json -o exe/sunshine-%npm_package_version%"
    },
    "bin": "server.js",
    "pkg": {
        "assets": [
            "dist/**/*"
        ],
        "outputPath": "exe",
        "targets": [
            "node18-linux-x64",
            "node18-win-x64"
        ]
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^16.1.4",
        "@angular/cdk": "^16.1.4",
        "@angular/common": "^16.1.4",
        "@angular/compiler": "^16.1.4",
        "@angular/core": "^16.1.4",
        "@angular/forms": "^16.1.4",
        "@angular/material": "^16.1.4",
        "@angular/platform-browser": "^16.1.4",
        "@angular/platform-browser-dynamic": "^16.1.4",
        "@angular/router": "^16.1.4",
        "bootstrap": "^5.2.3",
        "express": "^4.18.2",
        "pkg": "^5.8.1",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.13.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^16.1.4",
        "@angular/cli": "~16.1.4",
        "@angular/compiler-cli": "^16.1.4",
        "@types/jasmine": "~4.3.0",
        "jasmine-core": "~4.5.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.0.0",
        "typescript": "~4.9.4"
    }
}
